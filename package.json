{
  "name": "socketexpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "debug": "nodemon ./bin/www.js --delay 1500ms",
    "build": "flow-remove-types src/ -d lib/",
    "buildbabdel": "babel src/ --out-dir dist/ --no-babelrc --presets=flow --ignore spec.js ",
    "dev": " concurrently  \"npm run buildbabdel -- --watch\" \"nodemon dist/server.js\"",
    "killnode": "killall -s KILL node",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.1",
    "socket.io": "^2.2.0",
    "socketio-jwt": "^4.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^4.1.1",
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "flow-bin": "^0.105.1",
    "flow-remove-types": "^2.105.0",
    "nodemon": "^1.19.1"
  }
}
